{
  "name": "gestionix-boa",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --modern",
    "build:analyze": "cross-env RUN_ANALYZER=true vue-cli-service build --modern",
    "test": "run-s test:unit test:e2e",
    "lint": "run-s lint:all:*",
    "build:ci": "yarn build --report",
    "dev": "vue-cli-service serve",
    "dev:e2e": "vue-cli-service test:e2e --mode=development",
    "docs": "vuepress dev",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "new": "hygen new",
    "storybook": "start-storybook --port 60917",
    "storybook:build": "build-storybook",
    "test:ci": "run-s test:unit:ci test:e2e",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change"
  },
  "dependencies": {
    "@bugsnag/js": "^6.3.2",
    "@bugsnag/plugin-vue": "^6.3.0",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^6.1.0",
    "@testing-library/vue": "^4.1.0",
    "animejs": "^3.1.0",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "auth0-js": "^9.10.4",
    "axios": "0.18.0",
    "b64-to-blob": "^1.2.19",
    "croppie": "^2.6.4",
    "cypress": "4.3.0",
    "cypress-cucumber-preprocessor": "^2.2.0",
    "date-fns": "^1.30.1",
    "dompurify": "^2.0.7",
    "file-saver": "^1.3.8",
    "glider-js": "^1.6.6",
    "lodash": "4.17.11",
    "lodash.assign": "^4.2.0",
    "luxon": "^1.17.2",
    "marked": "^0.7.0",
    "material-design-icons": "^3.0.1",
    "normalize.css": "8.0.1",
    "pdfjs-dist": "^2.0.943",
    "popper.js": "^1.14.7",
    "portal-vue": "^2.0.0-beta.3",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.2",
    "vue-cropperjs": "^4.0.0",
    "vue-datetime": "^1.0.0-beta.10",
    "vue-intercom": "^1.0.14",
    "vue-meta": "1.5.8",
    "vue-mq": "^1.0.1",
    "vue-multiselect": "^2.1.6",
    "vue-router": "3.0.2",
    "vue-virtual-scroll-list": "^1.4.2",
    "vuelidate": "^0.7.4",
    "vuex": "3.1.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "weekstart": "^1.0.0",
    "xstate": "^4.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-storysource": "^5.0.5",
    "@storybook/addon-viewport": "5.0.5",
    "@storybook/vue": "^5.0.5",
    "@vue/cli-plugin-babel": "3.4.x",
    "@vue/cli-plugin-e2e-cypress": "4.3.0",
    "@vue/cli-plugin-eslint": "3.4.x",
    "@vue/cli-plugin-pwa": "4.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "3.4.x",
    "@vue/cli-service": "3.4.x",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/eslint-config-standard": "4.0.x",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.x",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "5.2.x",
    "cypress-plugin-retries": "^1.5.2",
    "eslint-plugin-vue": "5.1.x",
    "express": "4.16.x",
    "graphql-tag": "^2.9.0",
    "hygen": "2.1.2",
    "lint-staged": "8.1.x",
    "markdownlint-cli": "0.13.x",
    "node-sass": "4.13.x",
    "npm-run-all": "4.1.x",
    "prettier": "1.16.x",
    "purgecss": "^1.2.0",
    "sass-loader": "7.1.x",
    "storybook-addon-vue-info": "^1.0.1",
    "storybook-vue-router": "^1.0.3",
    "stylelint": "9.10.x",
    "stylelint-config-css-modules": "1.3.x",
    "stylelint-config-prettier": "4.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.2.x",
    "stylelint-scss": "3.5.x",
    "tailwindcss": "1.2.0",
    "vue-cli-plugin-apollo": "^0.20.0",
    "vue-loader": "^15.7.0",
    "vue-reactive-provide": "^0.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "0.14.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "resolutions": {
    "@vue/cli-plugin-unit-jest/jest": "^24.6.0",
    "@vue/cli-plugin-unit-jest/babel-jest": "^24.6.0",
    "vue-cropperjs/cropperjs": "^1.5.6",
    "@vue/cli-plugin-e2e-cypress/cypress": "^4.3.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "step_definitions" : "tests/e2e/specs"
  }
}
